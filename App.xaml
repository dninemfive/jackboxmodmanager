<Application x:Class="JackboxModManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:JackboxModManager"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundDark" Color="#111111"/>
        <SolidColorBrush x:Key="BackgroundLight" Color="#333333"/>
        <SolidColorBrush x:Key="Foreground" Color="#AAAAAA"/>
        <SolidColorBrush x:Key="Highlight" Color="#EE6900"/>
        <Thickness x:Key="Margin" Left="4" Top="4" Right="4" Bottom="4"/>
        <Thickness x:Key="MenuItem" Left="6" Top="1" Right="6" Bottom="1"/>
    </Application.Resources>
</Application>
